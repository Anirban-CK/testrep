  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/node": "^24.9.2",
    "@types/uuid": "^10.0.0",
    "allure-commandline": "^2.34.1",
    "allure-playwright": "^3.4.2",
    "dotenv": "^17.2.3",
    "husky": "^9.1.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "dependencies": {
    "uuid": "^13.0.0"
  }
